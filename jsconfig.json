{
  // 编译选项
  "compilerOptions": {
    // // 编译输出目标 ES 版本
    // "target": "esnext",
    // // 采用的模块系统
    // "module": "esnext",
    // // 以严格模式解析
    // "strict": true,
    // "jsx": "preserve",
    // // 如何处理模块
    // "moduleResolution": "node",
    // // 启用装饰器
    // "experimentalDecorators": true,
    // "skipLibCheck": true,
    // "esModuleInterop": true,
    // // 允许从没有设置默认导出的模块中默认导入
    // "allowSyntheticDefaultImports": true,
    // // 是否包含可以用于 debug 的 sourceMap
    // "sourceMap": true,
    // 解析非相对模块名的基准目录
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@crud/*": ["src/components/Crud/*"],
      "@comp/*": ["src/components/*"],
      "@comp-common/*": ["src/components-system/common/*"],
      "@comp-base/*": ["src/components-system/base/*"],
      "@comp-wms/*": ["src/components-system/wms/*"],
      "@comp-cls/*": ["src/components-system/classification/*"],
      "@comp-mes/*": ["src/components-system/mes/*"],
      "@comp-label/*": ["src/components-system/label/*"],
      "@compos/*": ["src/composables/*"],
      "@enum": ["src/utils/enum"],
      "@enum-ms/*": ["src/utils/enum/modules/*"],
      "@data-type/*": ["src/utils/data-type/*"]
    }
  }
}
